{
    "name": "TwitterExt",
    "manifest_version": 3,
    "version": "0.01",
    "description": "Twitter Extension in Chrome",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "host_permissions": ["<all_urls>"],
    "permissions": ["scripting"],
    "background": {
        "service_worker": "background.js"
    }
  }